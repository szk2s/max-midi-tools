{
  "env": {
      "es6": true,
      "node": true
  },
  "parser": "babel-eslint",
  "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": 2015
  },
  "plugins": [
      "prettier"
  ],
  "rules": {
      "no-console": "off",
      "prettier/prettier": "error"
  },
  "globals": {
      "test": "readonly",
      "beforeEach": "readonly",
      "maxAPI": "readonly"
  }
}
